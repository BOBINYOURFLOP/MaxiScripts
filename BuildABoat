-- Scripts:

local function ONPZFHX_fake_script() -- TextLabel.LocalScript 
	local script = Instance.new('LocalScript', TextLabel)

	function Farmed()
		Now = game:GetService("Players").Maxi1230111.Data.Gold.Value
		Later = game:GetService("Players").Maxi1230111.Data.Gold.Value
	while true do
		wait()
		Later = game:GetService("Players").Maxi1230111.Data.Gold.Value
		GoldGetted = Later - Now
		script.Parent.Text = "Gold Farmed : "..GoldGetted
		end
		
	end
	
	function Reset()
		script.Parent.Parent.TextButton2.MouseButton1Click:Connect(function()
			spawn(Farmed)
		end)
	end
	
	spawn(Reset)
	spawn(Farmed)
end
coroutine.wrap(ONPZFHX_fake_script)()
