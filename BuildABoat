local function FIAFEYF_fake_script() -- TextButton_4.LocalScript 
	local script = Instance.new('LocalScript', TextButton_4)

	Farming = true
	script.Parent.MouseButton1Click:Connect(function()
		if Farming == true then
			script.Parent.Parent.TrueOrFalse.Visible = true
			FarmStart = true
			wait(0.1)
			spawn(Farm)
			Farming = false
		else
			if Farming == false  then
				script.Parent.Parent.TrueOrFalse.Visible = false
				FarmStart = false
				Farming =  true
			end
		end
	end)
	
	FarmCompledetChest = 0
	function Farm()
		if FarmStart == true then
			local CFrameEnd = CFrame.new(-75.643836975098, 45.473701477051, 2124.0078125) -- Place your coords in here
			local Time = 2 -- Time in seconds
			local tween =  game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(Time), {CFrame = CFrameEnd})
			tween:Play()
			wait(1)
			local CFrameEnd = CFrame.new(-51.5656433, 65.0000458, 2139.09009, 1, 0, 0, 0, 1, 0, 0, 0, 1) -- Place your coords in here
			local Time = 4 -- Time in seconds
			local tween =  game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(Time), {CFrame = CFrameEnd})
			tween:Play()
			wait(2)
			local CFrameEnd = CFrame.new(-55.7065125, -358.739624, 9492.35645, 0, 0, -1, 0, 1, 0, 1, 0, 0) -- Place your coords in here
			local Time = 0 -- Time in seconds
			local tween =  game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(Time), {CFrame = CFrameEnd})
			tween:Play()
			wait(4)
			workspace.ClaimRiverResultsGold:FireServer()
			wait(15)
			FarmCompledetChest += 1
			print("Farm Compledet "..FarmCompledetChest.." Times!")
			spawn(Farm)
		end
	end
end
coroutine.wrap(FIAFEYF_fake_script)()
local function DNAMU_fake_script() -- TextButton_5.LocalScript 
	local script = Instance.new('LocalScript', TextButton_5)

	Farming = true
	script.Parent.MouseButton1Click:Connect(function()
		if Farming == true then
			script.Parent.Parent.TrueOrFalse2.Visible = true
			FarmStart = true
			wait(0.1)
			spawn(Farm2)
			Farming = false
		else
			if Farming == false  then
				script.Parent.Parent.TrueOrFalse2.Visible = false
				FarmStart = false
				Farming =  true
			end
		end
	end)
	
	FarmCompledetNormal = 0
	function Farm2()
		if FarmStart == true then
			local CFrameEnd = CFrame.new(-54.936786651611, 75.205612182617, 1314.0385742188) -- Place your coords in here
			local Time = 0 -- Time in seconds
			local tween =  game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(Time), {CFrame = CFrameEnd})
			tween:Play()
			tween.Completed:Wait()
			local CFrameEnd = CFrame.new(-54.211936950684, 75.125732421875, 8655.7119140625) -- Place your coords in here
			local Time = 37 -- Time in seconds
			local tween =  game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(Time), {CFrame = CFrameEnd})
			tween:Play()
			tween.Completed:Wait()
			local CFrameEnd = CFrame.new(-57.774879455566, -358.12963867188, 9498.6650390625) -- Place your coords in here
			local Time = 5 -- Time in seconds
			local tween =  game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(Time), {CFrame = CFrameEnd})
			tween:Play()
			tween.Completed:Wait()
			wait(4)
			workspace.ClaimRiverResultsGold:FireServer()
			wait(15)
			FarmCompledetNormal += 1
			print("Farm Compledet "..FarmCompledetNormal.." Times!")
			spawn(Farm2)
		end
	end
end
