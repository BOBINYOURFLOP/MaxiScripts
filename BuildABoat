local function FYVDSBC_fake_script() -- Script.LocalScript 
	local script = Instance.new('LocalScript', Script)

	local vu = game:GetService("VirtualUser")
	game:GetService("Players").LocalPlayer.Idled:connect(function()
		vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
		wait(1)
		vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
	end)
end
coroutine.wrap(FYVDSBC_fake_script)()
local function FUZTBR_fake_script() -- TextButton_2.LocalScript 
	local script = Instance.new('LocalScript', TextButton_2)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.Parent.Parent.load:Destroy()
	end)
end
coroutine.wrap(FUZTBR_fake_script)()
local function BTQSCW_fake_script() -- TextButton_3.LocalScript 
	local script = Instance.new('LocalScript', TextButton_3)

	
	Set = true
	script.Parent.MouseButton1Click:Connect(function()
		if Set == true then
			script.Parent.Parent.Parent.OtherFarms.Visible = true
			Set = false
		else
			if Set == false  then
				script.Parent.Parent.Parent.OtherFarms.Visible = false
				Set = true
			end
		end
	end)
end
